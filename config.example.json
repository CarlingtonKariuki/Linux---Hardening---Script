{
    "firewall": {
        "enable_ufw": true,
        "default_policy": "deny",
        "allow_ports": [22, 80, 443]
    },
    "services": {
        "disable": ["avahi-daemon", "cups", "rpcbind"]
    },
    "filesystem": {
        "protect_files": ["/etc/passwd", "/etc/shadow"],
        "audit_logs": true
    }
}

